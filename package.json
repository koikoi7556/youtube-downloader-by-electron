{
  "name": "YoutubeDownloder",
  "version": "1.0.0",
  "description": "Youtubeの動画をDLできるGUIソフト",
  "main": "src/main/index.js",
  "scripts": {
    "electron": "electron .",
    "server": "webpack-dev-server",
    "watch": "webpack --watch --mode=development --devtool source-map",
    "dist": "webpack --mode=production && electron-builder",
    "pack": "electron-builder --dir"
  },
  "author": {
    "name": "koijammer",
    "email": "koikoi7556@gmail.com",
    "url": "https://twitter.com/koijammer"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^3.5.3",
    "electron": "^9.0.0",
    "electron-builder": "^22.6.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "ffmpeg-static": "^4.2.2",
    "fluent-ffmpeg": "^2.1.2",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "open": "^7.0.4",
    "popper.js": "^1.16.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "ytdl-core": "^3.1.0"
  },
  "build": {
    "appId": "com.electron.yourapp",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/main/*",
      "app",
      "package.json"
    ],
    "win": {
      "icon": "resources/icon.ico",
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
